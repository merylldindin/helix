{
  "name": "helix",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nuxi generate",
    "checks": "yarn prettier & yarn stylelint & yarn eslint & yarn types &",
    "clean": "nuxi cleanup",
    "cypress": "cypress run --browser chrome",
    "cypress:open": "cypress open --browser chrome",
    "eslint": "eslint \"src/**/*.{ts,js,vue}\" --no-fix",
    "eslint:fix": "eslint \"src/**/*.{ts,js,vue}\" --fix",
    "prepare": "husky && yarn nuxt prepare",
    "prettier": "prettier --check --log-level error \"src/**/*.{ts,js,scss,css,vue,json}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,js,scss,css,vue,json}\"",
    "serve": "nuxi preview -o",
    "setup": "husky init && rm .husky/pre-commit && chmod ug+x .husky/*",
    "start": "nuxi dev -o",
    "stylelint": "npx stylelint 'src/**/*.{vue,scss}'",
    "stylelint:fix": "npx stylelint 'src/**/*.{vue,scss}' --fix",
    "types": "nuxi typecheck"
  },
  "resolutions": {
    "@nuxt/kit": ">=3.16.0",
    "micromatch": ">=4.0.8",
    "semver": ">=7.7.1",
    "tough-cookie": ">=5.1.2"
  },
  "dependencies": {
    "lodash": "4.17.21",
    "nuxt-jsonld": "2.2.1",
    "nuxt-swiper": "2.0.1",
    "vue3-lottie": "3.3.1",
    "vuetify": "3.9.2"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@mdi/js": "7.4.47",
    "@nuxt/devtools": "2.6.2",
    "@nuxt/eslint": "1.7.1",
    "@nuxt/eslint-config": "1.7.1",
    "@nuxt/types": "2.18.1",
    "@nuxt/vue-app": "2.18.1",
    "@nuxtjs/sitemap": "7.4.3",
    "@types/eslint": "9.6.1",
    "@types/lodash": "4.17.20",
    "@types/node": "22.16.5",
    "@typescript-eslint/parser": "8.38.0",
    "@unhead/addons": "2.0.12",
    "@vue/compiler-sfc": "3.5.18",
    "cypress": "14.5.3",
    "eslint": "9.32.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.3",
    "eslint-plugin-unicorn": "60.0.0",
    "eslint-plugin-vue": "10.3.0",
    "globals": "16.3.0",
    "husky": "9.1.7",
    "nuxt": "4.0.1",
    "nuxt-link-checker": "4.3.1",
    "nuxt-unhead": "1.4.20",
    "postcss-html": "1.8.0",
    "postcss-scss": "4.0.9",
    "prettier": "3.6.2",
    "prettier-plugin-packagejson": "2.5.19",
    "rollup": "4.45.3",
    "sass": "1.89.2",
    "serve": "14.2.4",
    "stylelint": "16.22.0",
    "stylelint-config-recommended-scss": "15.0.1",
    "stylelint-config-recommended-vue": "1.6.1",
    "stylelint-config-standard-scss": "15.0.1",
    "stylelint-prettier": "5.0.3",
    "typescript": "5.8.3",
    "vite": "7.0.6",
    "vue": "3.5.18",
    "vue-gtag-next": "1.14.0",
    "vue-tsc": "3.0.4"
  },
  "engines": {
    "node": "22.14.0",
    "yarn": "4.7.0"
  },
  "packageManager": "yarn@4.7.0"
}
