{
  "name": "helix",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nuxi generate",
    "checks": "yarn prettier & yarn stylelint & yarn eslint & yarn types &",
    "clean": "nuxi cleanup",
    "cypress": "cypress run --browser chrome",
    "cypress:open": "cypress open --browser chrome",
    "eslint": "eslint \"src/**/*.{ts,js,vue}\" --no-fix",
    "eslint:fix": "eslint \"src/**/*.{ts,js,vue}\" --fix",
    "prepare": "nuxi prepare",
    "prettier": "prettier --check --log-level error \"src/**/*.{ts,js,scss,css,vue,json}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,js,scss,css,vue,json}\"",
    "serve": "nuxi preview -o",
    "setup": "husky init && rm .husky/pre-commit && chmod ug+x .husky/*",
    "start": "nuxi dev -o",
    "stylelint": "npx stylelint 'src/**/*.{vue,scss}'",
    "stylelint:fix": "npx stylelint 'src/**/*.{vue,scss}' --fix",
    "types": "nuxi typecheck"
  },
  "resolutions": {
    "semver": ">= 7.5.2",
    "tough-cookie": ">= 4.1.3"
  },
  "dependencies": {
    "lodash": "4.17.21",
    "nuxt-jsonld": "2.1.0",
    "nuxt-swiper": "1.2.2",
    "vue3-lottie": "3.3.1",
    "vuetify": "3.7.9"
  },
  "devDependencies": {
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@mdi/js": "7.4.47",
    "@nuxt/devtools": "1.7.0",
    "@nuxt/eslint": "1.0.0",
    "@nuxt/eslint-config": "1.0.0",
    "@nuxt/types": "2.18.1",
    "@nuxt/vue-app": "2.18.1",
    "@nuxtjs/sitemap": "7.2.4",
    "@types/eslint": "9.6.1",
    "@types/lodash": "^4.17.6",
    "@types/node": "22.13.0",
    "@typescript-eslint/parser": "8.22.0",
    "@unhead/addons": "1.11.18",
    "@vue/compiler-sfc": "3.5.13",
    "cypress": "14.0.1",
    "eslint": "9.19.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-unicorn": "56.0.1",
    "eslint-plugin-vue": "9.32.0",
    "globals": "15.14.0",
    "husky": "9.1.7",
    "nuxt": "3.15.4",
    "nuxt-link-checker": "4.1.0",
    "nuxt-unhead": "1.4.20",
    "postcss-html": "1.8.0",
    "postcss-scss": "4.0.9",
    "prettier": "3.4.2",
    "prettier-plugin-packagejson": "2.5.8",
    "rollup": "4.34.0",
    "sass": "1.83.4",
    "serve": "14.2.4",
    "stylelint": "16.14.1",
    "stylelint-config-recommended-scss": "14.1.0",
    "stylelint-config-recommended-vue": "1.6.0",
    "stylelint-config-standard-scss": "14.0.0",
    "stylelint-prettier": "5.0.3",
    "typescript": "5.7.3",
    "vite": "6.0.11",
    "vue": "3.5.13",
    "vue-gtag-next": "1.14.0",
    "vue-tsc": "2.2.0"
  },
  "engines": {
    "node": "20.14.0",
    "yarn": "4.3.1"
  },
  "packageManager": "yarn@4.3.1"
}
