{
  "name": "helix",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nuxi generate",
    "clean": "nuxi cleanup && nuxi prepare",
    "cypress": "cypress run --browser chrome",
    "cypress:open": "cypress open --browser chrome",
    "eslint": "eslint \"src/**/*.{ts,js,vue}\" --no-fix",
    "eslint:fix": "eslint \"src/**/*.{ts,js,vue}\" --fix",
    "prepare": "husky",
    "prettier": "prettier --check --log-level error \"src/**/*.{ts,js,scss,css,vue,json}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,js,scss,css,vue,json}\"",
    "serve": "serve dist/",
    "setup": "husky init && rm .husky/pre-commit && chmod ug+x .husky/*",
    "start": "nuxi dev -o",
    "stylelint": "npx stylelint 'src/**/*.{vue,scss}'",
    "stylelint:fix": "npx stylelint 'src/**/*.{vue,scss}' --fix",
    "types": "tsc --noEmit"
  },
  "resolutions": {
    "semver": ">= 7.5.2",
    "tough-cookie": ">= 4.1.3"
  },
  "dependencies": {
    "nuxt-jsonld": "2.0.8",
    "nuxt-swiper": "1.2.2",
    "vue3-lottie": "3.3.0",
    "vuetify": "3.6.8"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@mdi/js": "7.4.47",
    "@nuxt/devtools": "1.3.2",
    "@nuxt/eslint": "0.3.13",
    "@nuxt/eslint-config": "0.3.13",
    "@nuxt/types": "2.17.3",
    "@nuxt/vue-app": "2.17.3",
    "@types/node": "20.14.0",
    "@typescript-eslint/parser": "7.11.0",
    "@unhead/addons": "1.9.12",
    "@vue/compiler-sfc": "3.4.27",
    "cypress": "13.10.0",
    "eslint": "9.4.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-unicorn": "53.0.0",
    "eslint-plugin-vue": "9.26.0",
    "husky": "9.0.11",
    "nuxt": "3.11.2",
    "nuxt-link-checker": "2.1.11",
    "nuxt-simple-sitemap": "4.4.1",
    "nuxt-unhead": "1.4.20",
    "postcss-html": "1.7.0",
    "postcss-scss": "4.0.9",
    "prettier": "3.3.0",
    "prettier-plugin-packagejson": "2.5.0",
    "sass": "1.77.4",
    "serve": "14.2.3",
    "stylelint": "16.6.1",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recommended-scss": "14.0.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard-scss": "13.1.0",
    "stylelint-prettier": "5.0.0",
    "typescript": "5.4.5",
    "vue-gtag-next": "1.14.0",
    "vue-tsc": "2.0.19"
  },
  "packageManager": "yarn@4.1.1"
}
