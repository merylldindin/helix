{
  "name": "helix",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nuxi generate",
    "checks": "yarn prettier & yarn stylelint & yarn eslint & yarn types &",
    "clean": "nuxi cleanup",
    "cypress": "cypress run --browser chrome",
    "cypress:open": "cypress open --browser chrome",
    "eslint": "eslint \"src/**/*.{ts,js,vue}\" --no-fix",
    "eslint:fix": "eslint \"src/**/*.{ts,js,vue}\" --fix",
    "prepare": "nuxi prepare",
    "prettier": "prettier --check --log-level error \"src/**/*.{ts,js,scss,css,vue,json}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,js,scss,css,vue,json}\"",
    "serve": "nuxi preview -o",
    "setup": "husky init && rm .husky/pre-commit && chmod ug+x .husky/*",
    "start": "nuxi dev -o",
    "stylelint": "npx stylelint 'src/**/*.{vue,scss}'",
    "stylelint:fix": "npx stylelint 'src/**/*.{vue,scss}' --fix",
    "types": "nuxi typecheck"
  },
  "resolutions": {
    "semver": ">= 7.5.2",
    "tough-cookie": ">= 4.1.3"
  },
  "dependencies": {
    "lodash": "4.17.21",
    "nuxt-jsonld": "2.0.8",
    "nuxt-swiper": "1.2.2",
    "vue3-lottie": "3.3.1",
    "vuetify": "3.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@mdi/js": "7.4.47",
    "@nuxt/devtools": "1.5.1",
    "@nuxt/eslint": "0.5.7",
    "@nuxt/eslint-config": "0.5.7",
    "@nuxt/types": "2.18.1",
    "@nuxt/vue-app": "2.18.1",
    "@nuxtjs/sitemap": "6.1.1",
    "@types/eslint": "9.6.1",
    "@types/lodash": "^4.17.6",
    "@types/node": "20.16.10",
    "@typescript-eslint/parser": "8.7.0",
    "@unhead/addons": "1.11.6",
    "@vue/compiler-sfc": "3.5.10",
    "cypress": "13.15.0",
    "eslint": "9.11.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-unicorn": "55.0.0",
    "eslint-plugin-vue": "9.28.0",
    "globals": "15.9.0",
    "husky": "9.1.6",
    "nuxt": "3.13.2",
    "nuxt-link-checker": "3.1.1",
    "nuxt-unhead": "1.4.20",
    "postcss-html": "1.7.0",
    "postcss-scss": "4.0.9",
    "prettier": "3.3.3",
    "prettier-plugin-packagejson": "2.5.2",
    "rollup": "4.22.5",
    "sass": "1.79.4",
    "serve": "14.2.3",
    "stylelint": "16.9.0",
    "stylelint-config-recommended-scss": "14.1.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard-scss": "13.1.0",
    "stylelint-prettier": "5.0.2",
    "typescript": "5.6.2",
    "vite": "5.4.8",
    "vue": "3.5.10",
    "vue-gtag-next": "1.14.0",
    "vue-tsc": "2.1.6"
  },
  "engines": {
    "node": "20.14.0",
    "yarn": "4.3.1"
  },
  "packageManager": "yarn@4.3.1"
}
