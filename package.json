{
  "name": "helix",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "nuxi generate",
    "clean": "nuxi clean",
    "checks": "yarn prettier & yarn stylelint & yarn eslint & yarn types &",
    "cypress": "cypress run --browser chrome",
    "cypress:open": "cypress open --browser chrome",
    "eslint": "eslint \"src/**/*.{ts,js,vue}\" --no-fix",
    "eslint:fix": "eslint \"src/**/*.{ts,js,vue}\" --fix",
    "prepare": "nuxi prepare",
    "prettier": "prettier --check --loglevel error \"src/**/*.{ts,js,scss,css,vue,json}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,js,scss,css,vue,json}\"",
    "serve": "serve dist/",
    "setup": "husky install && chmod ug+x .husky/*",
    "start": "nuxi dev -o",
    "stylelint": "npx stylelint 'src/**/*.{vue,scss}'",
    "stylelint:fix": "npx stylelint 'src/**/*.{vue,scss}' --fix",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "vue3-lottie": "2.5.0",
    "vuetify": "3.1.7"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@mdi/js": "7.1.96",
    "@nuxt/eslint-config": "0.1.1",
    "@nuxt/types": "2.16.0",
    "@nuxt/vue-app": "2.16.0",
    "@types/node": "18.14.6",
    "@unhead/addons": "1.1.15",
    "cypress": "12.7.0",
    "eslint": "8.35.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-sonarjs": "0.18.0",
    "eslint-plugin-unicorn": "45.0.2",
    "eslint-plugin-vue": "9.9.0",
    "husky": "8.0.3",
    "nuxt": "3.2.3",
    "nuxt-link-checker": "1.0.3",
    "nuxt-schema-org": "2.1.2",
    "nuxt-simple-sitemap": "1.0.7",
    "nuxt-unhead": "1.4.11",
    "postcss-html": "1.5.0",
    "postcss-scss": "4.0.6",
    "prettier": "2.8.4",
    "prettier-plugin-packagejson": "2.4.3",
    "sass": "1.58.3",
    "serve": "14.2.0",
    "stylelint": "15.2.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recommended-scss": "9.0.1",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard-scss": "7.0.1",
    "stylelint-prettier": "3.0.0",
    "typescript": "4.9.5",
    "vue-gtag-next": "1.14.0",
    "vue-tsc": "1.2.0"
  },
  "packageManager": "yarn@3.4.1"
}
