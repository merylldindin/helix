{
  "name": "helix",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "nuxi generate",
    "checks": "yarn prettier & yarn stylelint & yarn eslint & yarn types &",
    "eslint": "eslint \"src/**/*.{ts,js,vue}\" --no-fix",
    "eslint:fix": "eslint \"src/**/*.{ts,js,vue}\" --fix",
    "prettier": "prettier --check --loglevel error \"src/**/*.{ts,js,scss,css,vue,json}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,js,scss,css,vue,json}\"",
    "serve": "serve dist/",
    "setup": "husky install && chmod ug+x .husky/*",
    "start": "nuxi dev -o",
    "stylelint": "npx stylelint 'src/**/*.{vue,scss}'",
    "stylelint:fix": "npx stylelint 'src/**/*.{vue,scss}' --fix",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "@mdi/font": "7.1.96",
    "sass": "1.57.1",
    "vuetify": "3.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@nuxt/eslint-config": "0.1.1",
    "@nuxt/types": "2.15.8",
    "@nuxt/ui": "0.4.0",
    "@nuxt/vue-app": "2.15.8",
    "@types/node": "18.11.18",
    "eslint": "8.33.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-sonarjs": "0.18.0",
    "eslint-plugin-unicorn": "45.0.2",
    "eslint-plugin-vue": "9.9.0",
    "husky": "8.0.3",
    "nuxt": "3.1.1",
    "postcss-html": "1.5.0",
    "postcss-scss": "4.0.6",
    "prettier": "2.8.3",
    "prettier-plugin-packagejson": "2.4.2",
    "serve": "14.2.0",
    "stylelint": "14.16.1",
    "stylelint-config-recommended-scss": "8.0.0",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard-scss": "6.1.0",
    "typescript": "4.9.5",
    "vite-plugin-vuetify": "1.0.2",
    "vue-tsc": "1.0.24"
  },
  "packageManager": "yarn@3.3.1"
}
