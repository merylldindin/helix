{
  "name": "helix",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "nuxi generate",
    "checks": "yarn prettier & yarn stylelint & yarn eslint & yarn types &",
    "eslint": "eslint \"src/**/*.{ts,js,vue}\" --no-fix",
    "eslint:fix": "eslint \"src/**/*.{ts,js,vue}\" --fix",
    "prettier": "prettier --check --loglevel error \"src/**/*.{ts,js,scss,css,vue,json}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,js,scss,css,vue,json}\"",
    "serve": "serve dist/",
    "setup": "husky install && chmod ug+x .husky/*",
    "start": "nuxi dev -o",
    "stylelint": "npx stylelint 'src/**/*.{vue,scss}'",
    "stylelint:fix": "npx stylelint 'src/**/*.{vue,scss}' --fix",
    "types": "tsc --noEmit"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@nuxt/ui": "0.4.0",
    "@nuxtjs/eslint-config-typescript": "12.0.0",
    "eslint": "8.32.0",
    "husky": "8.0.3",
    "nuxt": "3.0.0",
    "nuxt-purgecss": "2.0.0",
    "postcss-html": "1.5.0",
    "prettier": "2.8.3",
    "prettier-plugin-packagejson": "2.4.0",
    "serve": "14.1.2",
    "stylelint": "14.16.1",
    "stylelint-config-recommended-scss": "8.0.0",
    "stylelint-config-recommended-vue": "1.4.0",
    "typescript": "4.9.4",
    "vue-tsc": "1.0.24"
  },
  "packageManager": "yarn@3.3.1"
}
