{
  "name": "helix",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "nuxi generate",
    "checks": "yarn prettier & yarn stylelint & yarn eslint & yarn types &",
    "clean": "nuxi clean",
    "cypress": "cypress run --browser chrome",
    "cypress:open": "cypress open --browser chrome",
    "eslint": "eslint \"src/**/*.{ts,js,vue}\" --no-fix",
    "eslint:fix": "eslint \"src/**/*.{ts,js,vue}\" --fix",
    "prepare": "nuxi prepare",
    "prettier": "prettier --check --log-level error \"src/**/*.{ts,js,scss,css,vue,json}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,js,scss,css,vue,json}\"",
    "serve": "serve dist/",
    "setup": "husky install && chmod ug+x .husky/*",
    "start": "nuxi dev -o",
    "stylelint": "npx stylelint 'src/**/*.{vue,scss}'",
    "stylelint:fix": "npx stylelint 'src/**/*.{vue,scss}' --fix",
    "types": "tsc --noEmit"
  },
  "resolutions": {
    "semver": ">= 7.5.2",
    "tough-cookie": ">= 4.1.3"
  },
  "dependencies": {
    "nuxt-jsonld": "2.0.8",
    "nuxt-swiper": "1.2.0",
    "vue3-lottie": "3.0.0",
    "vuetify": "3.3.9"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.7",
    "@commitlint/config-conventional": "17.6.7",
    "@mdi/js": "7.2.96",
    "@nuxt/devtools": "0.7.0",
    "@nuxt/eslint-config": "0.1.1",
    "@nuxt/types": "2.17.1",
    "@nuxt/vue-app": "2.17.1",
    "@types/node": "18.17.0",
    "@typescript-eslint/parser": "6.1.0",
    "@unhead/addons": "1.1.32",
    "cypress": "12.17.2",
    "eslint": "8.45.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-plugin-unicorn": "48.0.0",
    "eslint-plugin-vue": "9.15.1",
    "husky": "8.0.3",
    "nuxt": "3.6.5",
    "nuxt-link-checker": "1.0.5",
    "nuxt-simple-sitemap": "3.0.12",
    "nuxt-unhead": "1.4.17",
    "postcss-html": "1.5.0",
    "postcss-scss": "4.0.6",
    "prettier": "3.0.0",
    "prettier-plugin-packagejson": "2.4.5",
    "sass": "1.64.1",
    "serve": "14.2.0",
    "stylelint": "15.10.2",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recommended-scss": "12.0.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard-scss": "10.0.0",
    "stylelint-prettier": "4.0.0",
    "typescript": "5.1.6",
    "vue-gtag-next": "1.14.0",
    "vue-tsc": "1.8.6"
  },
  "packageManager": "yarn@3.6.1"
}
