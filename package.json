{
  "name": "helix",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "nuxi generate",
    "checks": "yarn prettier & yarn stylelint & yarn eslint & yarn types &",
    "clean": "nuxi clean",
    "cypress": "cypress run --browser chrome",
    "cypress:open": "cypress open --browser chrome",
    "eslint": "eslint \"src/**/*.{ts,js,vue}\" --no-fix",
    "eslint:fix": "eslint \"src/**/*.{ts,js,vue}\" --fix",
    "prepare": "nuxi prepare",
    "prettier": "prettier --check --log-level error \"src/**/*.{ts,js,scss,css,vue,json}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,js,scss,css,vue,json}\"",
    "serve": "serve dist/",
    "setup": "husky install && chmod ug+x .husky/*",
    "start": "nuxi dev -o",
    "stylelint": "npx stylelint 'src/**/*.{vue,scss}'",
    "stylelint:fix": "npx stylelint 'src/**/*.{vue,scss}' --fix",
    "types": "tsc --noEmit"
  },
  "resolutions": {
    "semver": ">= 7.5.2",
    "tough-cookie": ">= 4.1.3"
  },
  "dependencies": {
    "nuxt-jsonld": "2.0.8",
    "nuxt-swiper": "1.2.2",
    "vue3-lottie": "3.2.0",
    "vuetify": "3.4.3"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@mdi/js": "7.3.67",
    "@nuxt/devtools": "1.0.3",
    "@nuxt/eslint-config": "0.2.0",
    "@nuxt/types": "2.17.2",
    "@nuxt/vue-app": "2.17.2",
    "@types/node": "20.10.0",
    "@typescript-eslint/parser": "6.12.0",
    "@unhead/addons": "1.8.8",
    "cypress": "13.6.0",
    "eslint": "8.54.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-unicorn": "49.0.0",
    "eslint-plugin-vue": "9.18.1",
    "husky": "8.0.3",
    "nuxt": "3.8.2",
    "nuxt-link-checker": "2.1.10",
    "nuxt-simple-sitemap": "4.1.6",
    "nuxt-unhead": "1.4.17",
    "postcss-html": "1.5.0",
    "postcss-scss": "4.0.9",
    "prettier": "3.1.0",
    "prettier-plugin-packagejson": "2.4.6",
    "sass": "1.69.5",
    "serve": "14.2.1",
    "stylelint": "15.11.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recommended-scss": "13.1.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard-scss": "11.1.0",
    "stylelint-prettier": "4.1.0",
    "typescript": "5.3.2",
    "vue-gtag-next": "1.14.0",
    "vue-tsc": "1.8.22"
  },
  "packageManager": "yarn@4.0.2"
}
